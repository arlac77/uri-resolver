{
  "name": "uri-resolver",
  "version": "0.0.0-semantic-release",
  "description": "uri resolver",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "module": "src/index.js",
  "scripts": {
    "cover": "npm run build && ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests",
    "test": "npm run build && mocha tests/*_test.js && markdown-doctest",
    "build": "./node_modules/rollup/bin/rollup --format=cjs --output=dist/index.js -- src/index.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t README.hbs -f src/index.js > README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/uri-resolver.git"
  },
  "keywords": [
    "uri",
    "resolver"
  ],
  "author": "Markus Felten <markus.felten@gmx.de>",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/uri-resolver/issues"
  },
  "homepage": "https://github.com/arlac77/uri-resolver#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "rollup": "^0.36.2",
    "semantic-release": "^6.3.1",
    "cz-conventional-changelog": "^1.2.0",
    "jsdoc-to-markdown": "^2.0.1",
    "markdown-doctest": "^0.9.0"
  },
  "dependencies": {
    "btoa": "^1.1.2",
    "https-proxy-agent": "^1.0.0",
    "node-fetch": "^1.6.3",
    "registry-mixin": "^2.1.0"
  },
  "engines": {
    "node": ">=6.9.1"
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}